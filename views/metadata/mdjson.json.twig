{% import "metadata/contactref.twig" as contactref %}
{% autoescape 'json' %}
{
  "schema": {
    "name": "mdJson",
    "version": "2.4.5"
  },
  "metadataRepository" : [{
      "metadataStandard" : "mdJSON",
      "citation" : {
        "title" : "LCC Network Science Catalog"
      },
      "repository" : "ScienceBase"
    }
    {% if not (resourceType == 'project') %}
    ,{ "metadataStandard" : "ISO 19115-2",
        "citation" : {
          "title" : "{{ organization.name }} data.gov"
        },
        "repository" : "data.gov"
      }
    {% endif %}
  ],
  "contact": [
    {
      "contactId": "c6007b64-4fbf-4a2d-ab79-8dc56bbdf0a2",
      "isOrganization": true,
      "name": "Landscape Conservation Cooperative Network",
      "logoGraphic": [{
        "fileUri": [{
          "name": "LCC-logo-med-100.jpg",
          "uri": "data:image/jpeg;base64,"
        }],
        "fileName": "LCC Network Logo"
      }],
      "phone": [{
        "service": ["voice"],
        "phoneName": "Elsa Haubold",
        "phoneNumber": "7033581953"
      }, {
        "service": ["voice"],
        "phoneName": "Jillian Cohen",
        "phoneNumber": "7033581764"
      }],
      "address": [{
        "addressType": ["mailing", "physical"],
        "deliveryPoint": ["Science Applications, USFWS", "5275 Leesburg Pike"],
        "city": "Falls Church",
        "adminstrativeArea": "VA",
        "postalCode": "22041",
        "country": "USA"
      }],
      "electronicMailAddress": ["elsa_haubold@fws.gov", "jillian_cohen@fws.gov"],
      "onlineResource": [{
        "name": "Landscape Conservation Cooperative Network Website",
        "uri": "https://lccnetwork.org"
      }],
      "contactType": "lcc"
    }, {
      "contactId": "edc6b779-3352-4a81-8430-76bcce1bfcb3",
      "isOrganization": false,
      "memberOfOrganization": [
        "c6007b64-4fbf-4a2d-ab79-8dc56bbdf0a2"
      ],
      "logoGraphic": [],
      "phone": [],
      "address": [],
      "electronicMailAddress": [
        "lccdatasteward@fws.gov"
      ],
      "onlineResource": [],
      "hoursOfService": [],
      "contactType": "lcc",
      "positionName": "LCC Network Data Manager",
      "name": "LCC Network Data Steward"
    },
  {% for contact in contacts %}
    {% include "metadata/contact.json.twig" %}
  {% endfor %}
],
  "metadata": {
    "metadataInfo": {% include "metadata/metadatainfo.json.twig" %},
    "resourceInfo": {% include "metadata/resourceinfo.json.twig" %},
    "resourceDistribution": [
      {% if links %}
      {
        "distributor": [
      	    {% include "metadata/distributioninfo.json.twig" %}
        ]
      }
      {% endif %}
  ],
    "associatedResource": [
	  {% for assoc in associated if assoc.resource.exportmetadata %}
	    {% if not loop.first %},{% endif %}
	    {% include "metadata/associated.json.twig" %}
	  {% endfor %}
    ],
    "additionalDocumentation": []
    {% if funding is defined  %}
    ,
    "funding":[
      {% for funds in funding %}
        {% if not loop.first %},{% endif %}
        {% include "metadata/funding.json.twig" %}
      {% endfor %}
    ]
    {% endif %}
  }
}
{% endautoescape %}
