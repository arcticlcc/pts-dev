{% autoescape 'json' %}
    {
      "contactId": "{{ contact.contactId }}",
      "individualName": "{{ contact.individualName }}",
      "organizationName": "{{ contact.organizationName }}",
      "positionName": "{{ contact.positionName }}"
      {% if contact.uri %}
      ,
      "onlineResource": [
        {
          "uri": "{{ contact.uri }}",
          "protocol": "http",
          "function": "information"
        }
      ]
      {% endif %}
      {% if contact.officePhone %}
      ,
      "phoneBook": [
        {
          "phoneNumber": "{{ contact.officePhone }}",
          "service": ["voice"]
        }
      ]
      {% endif %}
      {% if contact.deliveryPoint1 or contact.email %}
      ,
      "address": {
        {% if not contact.deliveryPoint1 is null %}
          {% set comma = true %}
        "deliveryPoint": ["{{ contact.deliveryPoint1 }}","{{ contact.deliveryPoint2 }}"],
        "city": "{{ contact.city }}",
        "administrativeArea": "{{ contact.administrativeArea }}",
        "postalCode": "{{ contact.postalCode }}",
        "country": "{{ contact.country }}"
        {% endif %}
        {% if not contact.email is null %}
          {{ comma is defined ? ',' : '' }}
        "electronicMailAddress": ["{{ contact.email }}"]
        {% endif %}
      }
      {% endif %}
    }{% if loop.last == false %},{% endif %}
{% endautoescape %}