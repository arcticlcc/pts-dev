{% macro ref(contacts, values, comma) %}
  {% for contact in contacts %}
    {% if contact.roletypeid in values %}
      {% if comma == true and loop.first %},{% endif %}
      {
        "contactId": "{{ contact.contactid }}",
        "role": "{{ contact.adiwg }}"
      }{% if loop.last == false %},{% endif %}
    {% endif %}
  {% endfor %}
{% endmacro %}