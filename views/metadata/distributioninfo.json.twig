{% autoescape 'json' %}
{
  "contact": {
      "party": [{"contactId": "{{ organization.uuid }}"}],
      "role": "owner"
  },
  "orderProcess": [
    {
      "fees": "none"
    }
  ],
  "transferOption": [
  {% for link in links %}
  {% if not loop.first %},{% endif %}
    {
      "onlineOption": [
        {
          "uri": "{{ link.uri }}",
          "name": "{{ link.title }}",
          "description": "{{ link.description }}",
          "function": "{{ link.function }}"
        }
      ]
    }
  {% endfor %}
  ]
}
{% endautoescape %}
