{% autoescape 'json' %}
{
  "contact": {
      "party": [{"contactId": "{{ organization.uuid }}"}],
      "role": "owner"
  },
  "orderProcess": [
    {
      "fees": "none"
    }
  ],
  "transferOption": [
  {% for link in links %}
    {% if link.onlinefunctionid in [1,3,4,11,12] %}
      {% if not loop.first %},{% endif %}
      {
        "onlineOption": [
          {
            "uri": "{{ link.uri }}",
            "name": "{{ link.title }}",
            "description": "{{ link.description }}",
            "function": "{{ link.function }}"
          }
        ]
      }
    {% endif %}
  {% endfor %}
  ]
}
{% endautoescape %}
